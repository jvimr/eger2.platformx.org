<h1>Listing test_pages</h1>

<table>
  <tr>
  <% for column in TestPage.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for test_page in @test_pages %>
  <tr>
  <% for column in TestPage.content_columns %>
    <td><%=h test_page.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => test_page %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => test_page %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => test_page }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @test_page_pages.current.previous } if @test_page_pages.current.previous %>
<%= link_to 'Next page', { :page => @test_page_pages.current.next } if @test_page_pages.current.next %> 

<br />

<%= link_to 'New test_page', :action => 'new' %>
