<h1>Listing test_types</h1>

<table>
  <tr>
  <% for column in TestType.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for test_type in @test_types %>
  <tr>
  <% for column in TestType.content_columns %>
    <td><%=h test_type.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => test_type %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => test_type %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => test_type }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @test_type_pages.current.previous } if @test_type_pages.current.previous %>
<%= link_to 'Next page', { :page => @test_type_pages.current.next } if @test_type_pages.current.next %> 

<br />

<%= link_to 'New test_type', :action => 'new' %>
